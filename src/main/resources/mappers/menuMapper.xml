<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
   <mapper namespace="com.petitdesert.page.menu.dao.MenuDAO">
   		<insert id="insertMenu" parameterType="map">
   		
	   		INSERT INTO 
				`menu`
			(
				`userId`
				, `menuName`
				, `price`
				, `introduce`
				, `imagePath`
				, `category`
				, `createdAt`
				, `updatedAt`
			)
			VALUE 
			(
				#{userId}
				, #{menuName}
				, #{price}
				, #{introduce}
				, #{imagePath}
				, #{category}
				, now()
				, now()
			)
   		
   		</insert>
   
   
   
   
   
   </mapper>